{
  "name": "@learn/agents",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "bun run dev:server & bun run dev:client",
    "dev:server": "bun run --hot server/src/server.ts",
    "start:server": "bun run server/src/server.ts",
    "dev:client": "sleep 2 && bun run --hot client/src/index.ts",
    "start:client": "bun run client/src/index.ts",
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "dependencies": {
    "@ai-sdk/google": "^3.0.23",
    "@ai-sdk/openai": "^3.0.26",
    "@clack/prompts": "^1.0.0",
    "ai": "^6.0.78",
    "googleapis": "^171.4.0",
    "hono": "^4.11.9",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/bun": "^1.3.8",
    "typescript": "^5.9.0"
  }
}
